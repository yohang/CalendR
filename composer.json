{
    "name": "yohang/calendr",
    "description": "Object Oriented calendar management",
    "keywords": [
        "date",
        "calendar",
        "events"
    ],
    "homepage": "https://github.com/yohang/CalendR",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yohan GIARELLI",
            "email": "yohan@giarel.li",
            "homepage": "http://yohan.giarel.li"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/options-resolver": ">=4.4,<8.0"
    },
    "require-dev": {
        "doctrine/common": "^3.1.2",
        "doctrine/orm": "^2.9.3|^3.0.0",
        "phpunit/phpunit": "^9.6.22|^10.5.40",
        "psr/simple-cache": "*",
        "symfony/dependency-injection": ">=4.4,<8.0",
        "symfony/config": ">=4.4,<8.0",
        "symfony/var-dumper": ">=4.4,<8.0",
        "symfony/yaml": ">=4.4,<8.0",
        "twig/twig": "^3.3.2",
        "phpspec/prophecy-phpunit": "^2.3.0",
        "symfony/cache": ">=5.3,<8.0",
        "symfony/http-kernel": ">=5.3,<8.0"
    },
    "autoload": {
        "psr-4": {
            "CalendR\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CalendR\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
