{
    "name": "yohang/calendr",
    "description": "Object Oriented calendar management",
    "keywords": [
        "date",
        "calendar",
        "events"
    ],
    "homepage": "https://github.com/yohang/CalendR",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yohan GIARELLI",
            "email": "yohan@giarel.li",
            "homepage": "http://yohan.giarel.li"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/options-resolver": "^4.4|^5.3"
    },
    "require-dev": {
        "doctrine/cache": "^2.1.1",
        "doctrine/common": "^3.1.2",
        "doctrine/orm": "^2.9.3",
        "phpunit/phpunit": "^8.0|^9.0",
        "symfony/dependency-injection": "^4.4|^5.3",
        "symfony/config": "^4.4|^5.3",
        "symfony/var-dumper": "^4.4|^5.3",
        "symfony/yaml": "^4.4|^5.3",
        "twig/twig": "^3.3.2",
        "phpspec/prophecy-phpunit": "^2.0",
        "symfony/cache": "^5.3",
        "symfony/http-kernel": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "CalendR\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CalendR\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
